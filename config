sudo openssl req -x509 -newkey rsa:4096 -sha256 -days 3650 -nodes -keyout artifactory.key -out artifactory.crt -extensions san -config <(echo "[req]"; echo distinguished_name=req; echo "[san]"; echo subjectAltName=DNS:artifactory.local,DNS:artifactory.net,IP:192.168.1.10) -subj /CN=artifactory.local


